{
  "name": "frostbugs-skin-testing-tools",
  "description": "Tools for testing CSGO skins for development and presentation purposes.",
  "version": "0.1.0",
  "homepage": "https://github.com/frostbug/frostbugs-skin-testing-tools#readme",
  "author": "Frostbug",
  "license": "GPL-2.0-only",
  "bugs": {
    "url": "https://github.com/frostbug/frostbugs-skin-testing-tools/issues"
  },
  "main": "public/Main.js",
  "keywords": [],
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "electron-start": "electron .",
    "preelectron-pack": "npm run build",
    "electron-pack": "electron-builder -c.extraMetadata.main=build/Main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frostbug/frostbugs-skin-testing-tools.git"
  },
  "dependencies": {
    "@craco/craco": "^6.4.2",
    "@electron/remote": "^2.0.1",
    "@node-steam/vdf": "^2.2.0",
    "@popperjs/core": "^2.10.2",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "bootstrap": "^5.1.3",
    "bootswatch": "^5.1.3",
    "jquery": "^3.6.0",
    "popper.js": "^1.16.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "remote": "^0.2.6",
    "web-vitals": "^1.1.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.38",
    "electron": "^15.3.1",
    "electron-builder": "^22.13.1",
    "typescript": "^4.4.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "nsis": {
      "artifactName": "Frostbugs-skin-testing-tools.setup.${ext}"
    },
    "appId": "nonExistentID",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
